msgid ""
"                                      la densification s'accroît, et la "
"géométrie s'affine aussi."
msgstr "la densificazione aumenta e anche la geometria viene perfezionata."

#: AperoDeDenis.py:3111
#| msgid ""
#| "                                La production du nuage de point non dense "
#| "est optionnelle."
msgid ""
